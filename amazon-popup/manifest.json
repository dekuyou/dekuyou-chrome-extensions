    "default_icon": "favicon.ico",
    "popup": "popup.html"
  },
  "background_page": "background.html",
  "permissions": ["tabs","http://*/*","https://*/*"],
   "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["jp_ddo_dekuyou_popAmazon.js"]
    }
  ]
}